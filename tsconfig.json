{
  "compilerOptions": {
    "module": "commonjs",
    "target": "ES2020",
    "sourceMap": true,
    "jsx": "react-jsx",
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "strictNullChecks": true,
    "lib": ["dom"],
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "typeRoots": ["types.d.ts", "node_modules/@types"]
  },
  "exclude": ["node_modules"]
}
